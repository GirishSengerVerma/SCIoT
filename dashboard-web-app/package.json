{
	"name": "dashboard-web-app",
	"version": "0.0.1",
	"description": "Dashboard Web App for monitoring and controlling weather events, sensors, actuators, etc.",
	"author": "SCIoT SS22 Group 30",
	"license": "MIT",
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"package": "svelte-kit package",
		"preview": "concurrently \"run-func ./services/dataService.js initializeDataService\" \"svelte-kit preview\"",
		"prepare": "svelte-kit sync",
		"start": "node ./server",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint .",
		"format": "prettier --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@babichjacob/svelte-localstorage": "^0.2.0",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/adapter-node": "^1.0.0-next.78",
		"@sveltejs/kit": "next",
		"@types/node": "^17.0.42",
		"@typescript-eslint/eslint-plugin": "^5.10.1",
		"@typescript-eslint/parser": "^5.10.1",
		"autoprefixer": "^10.4.7",
		"concurrently": "^7.2.2",
		"eslint": "^8.12.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss": "^8.4.14",
		"prettier": "^2.5.1",
		"prettier-plugin-svelte": "^2.5.0",
		"prisma": "^3.15.2",
		"run-func": "^3.0.0",
		"svelte": "^3.44.0",
		"svelte-check": "^2.2.6",
		"svelte-preprocess": "^4.10.7",
		"tailwindcss": "^3.1.3",
		"ts-node": "^10.8.1",
		"tslib": "^2.3.1",
		"typescript": "~4.6.2"
	},
	"type": "module",
	"dependencies": {
		"@prisma/client": "^3.15.2",
		"@types/express": "^4.17.13",
		"apexcharts": "^3.35.3",
		"dotenv": "^16.0.1",
		"express": "^4.18.1",
		"mqtt": "^4.3.7",
		"socket.io": "^4.5.1",
		"socket.io-client": "^4.5.1"
	}
}
